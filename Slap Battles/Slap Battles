Badges:Toggle{
	Name = "Jet Farm",
	StartingState = false,
	Description = 'Takes flight orbs whenever they spawn, giving the "Orb of flight" badge.',
	Callback = function(Value) 
		getgenv().jetFarm = Value
		while getgenv().jetFarm do
			for i,v in pairs(workspace:GetChildren()) do
                    		if v.Name == "JetOrb" then
					firetouchinterest(User.Character:WaitForChild("HumanoidRootPart"), v, 0)
					firetouchinterest(User.Character:WaitForChild("HumanoidRootPart"), v, 1)
				end
			end
			wait()
		end
	end
}

Badges:Button{
	Name = "Get Kinetic",
	Description = 'Gives you the "Kinetically Charged" badge. (Inconsistent)',
	Callback = function() 
		if User.leaderstats.Slaps.Value < 870 then
			GUI:Notification{
			Title = "Error",
			Text = "You must have at least 870 slaps.",
			Duration = 3,
			Callback = function() end
			}
		elseif User.Character:FindFirstChild("entered") and User.leaderstats.Glove.Value ~= "Stun" then
			GUI:Notification{
			Title = "Error",
			Text = "You must be in the lobby.",
			Duration = 3,
			Callback = function() end
			}
		else
			GUI:Prompt{
			Followup = false,
			Title = "Get Kinetic",
			Text = "This will interrupt whatever you are doing. It is recommended to become invisible before use.",
			Buttons = {
			Yes = function()
				if not User.Character:FindFirstChild("entered") then
					fireclickdetector(workspace.Lobby.Stun.ClickDetector)
					firetouchinterest(User.Character:WaitForChild("HumanoidRootPart"), workspace.Lobby.Teleport1, 0)
					firetouchinterest(User.Character:WaitForChild("HumanoidRootPart"), workspace.Lobby.Teleport1, 1)
				end
				wait(0.1)
				User.Character.HumanoidRootPart.CFrame = workspace.SafeSpot.CFrame * CFrame.new(0, 19, 0) 
				wait(0.5)
				repeat task.wait()
				game.ReplicatedStorage.SelfKnockback:FireServer({["Force"] = -99})
				until User.Character:WaitForChild("KineticSlappedBadgeCounter").Value >= 100
				wait(0.5)
				repeat
				for i,v in pairs(game.Players:GetChildren()) do
				if v ~= User and User.Character:FindFirstChild("HumanoidRootPart") and v.Character then
					if v.Character:FindFirstChild("HumanoidRootPart") and v.Character.isInArena.Value == true and not v.Character:FindFirstChild("Reversed") and not v.Character:FindFirstChild("rock") and not v.Character:FindFirstChild("Counterd") and not v.Character:FindFirstChild("Mirage") and not usingParry(v) and v.Character.Ragdolled.Value == false then
						User.Character.HumanoidRootPart.CFrame = workspace.Arena.Plate.CFrame * CFrame.new(0, 2, 0)
						wait(0.3)
						User.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame * CFrame.new(0,-12,0)
						if User.Backpack:FindFirstChild("Stun") then
							User.Character.Humanoid:EquipTool(User.Backpack.Stun)
						end
						wait(0.2)
						game.ReplicatedStorage.StunR:FireServer(User.Character.Stun)
						User.Character.HumanoidRootPart.CFrame = workspace.SafeSpot.CFrame * CFrame.new(0, 19, 0)
						break
					end
				end
			end
			wait(9.7)
			until User.Character:WaitForChild("EMPStunBadgeCounter").Value >= 50
			end,
			No = function()
				return
			end
			}
			}
		end
	end
}


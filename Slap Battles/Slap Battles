Protection:Toggle{
	Name = "Anti [REDACTED]",
	StartingState = false,
	Description = "Stops [REDACTED]'s ability from affecting you.",
	Callback = function(Value) 
		User.PlayerScripts.LegacyClient.Well.Disabled = Value
	end

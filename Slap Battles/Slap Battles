Protection:Toggle{
	Name = "Anti Ice",
	StartingState = false,
	Description = "Stops Ice's ability from affecting you.",
	Callback = function(Value) 
		getgenv().antiIce = Value
		while getgenv().antiIce do
			for i,v in pairs(User.Character:GetChildren()) do
				if v.Name == "Icecube" then
					v:Destroy()

					v.Enabled = false
				end
			end
			for i,v in pairs(User.PlayerGui:GetChildren()) do
				if v.Name:find("whiteframe") then
					v.Enabled = false
				end
			end
			task.wait()
		end
		for i,v in pairs(User.PlayerGui:GetChildren()) do
			if v.Name:find("whiteframe") then
				v.Enabled = true
			end
		end
	end
}

